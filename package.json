{
  "name": "agentic-project",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:genkit": "genkit start",
    "dev": "genkit start -- tsx watch src/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"cd frontend && npm run dev\"",
    "build": "tsc",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "build:prod": "tsup",
    "start": "node dist/index.js",
    "deploy": "gcloud app deploy",
    "start:prod": "NODE_ENV=production NODE_OPTIONS='--max-old-space-size=512 --expose-gc' node dist/index.js",
    "start:script": "./start-prod.sh",
    "test": "jest",
    "evaluate:models": "NODE_OPTIONS='--loader tsx' evaluation/evaluate-models.ts"
  },
  "dependencies": {
    "@commercelayer/js-auth": "^7.1.2",
    "@commercelayer/sdk": "^7.4.1",
    "@genkit-ai/ai": "^1.27.0",
    "@genkit-ai/core": "^1.27.0",
    "@genkit-ai/dotprompt": "^0.9.12",
    "@genkit-ai/flow": "^0.6.0-dev.1",
    "@genkit-ai/google-cloud": "^1.27.0",
    "@genkit-ai/google-genai": "^1.27.0",
    "@genkit-ai/googleai": "^1.27.0",
    "@google-analytics/data": "^5.2.1",
    "@google-cloud/bigquery": "^8.1.1",
    "@google-cloud/pubsub": "^5.2.2",
    "@google-cloud/storage": "^7.18.0",
    "@google-cloud/vision": "^5.3.4",
    "@google/generative-ai": "^0.24.1",
    "@hono/node-server": "^1.19.9",
    "@hono/zod-validator": "^0.7.6",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@sanity/client": "^7.14.0",
    "@supabase/supabase-js": "^2.90.1",
    "dotenv": "^17.2.3",
    "hono": "^4.11.4",
    "jimp": "^1.6.0",
    "json-rules-engine": "^7.3.1",
    "moment": "^2.30.1",
    "nanoid": "^5.1.6",
    "node-fetch": "^3.3.2",
    "pdfjs-dist": "^5.4.530",
    "pino": "^10.2.0",
    "web-push": "^3.6.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/web-push": "^3.6.4",
    "concurrently": "^9.2.1",
    "genkit": "^1.27.0",
    "genkit-cli": "^1.27.0",
    "jest": "^29.0.0",
    "prettier": "3.6.2",
    "ts-jest": "^29.0.0",
    "tsup": "^8.5.1",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
